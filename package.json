{
  "name": "@settlin/mup-aws-beanstalk-v7",
  "version": "0.7.1",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --copy-files --source-maps",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^8.0.1",
    "mup": "^1.5.4",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "archiver": "^4.0.2",
    "aws-sdk": "^2.991.0",
    "axios": "^0.21.4",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "ejs": "^2.5.7",
    "lodash": "^4.17.21",
    "random-seed": "^0.3.0",
    "shell-escape": "^0.2.0",
    "ssh2": "^1.4.0",
    "uuid": "^3.1.0"
  }
}
