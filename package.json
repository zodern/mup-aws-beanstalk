{
  "name": "mup-aws-beanstalk",
  "version": "0.6.0-beta3",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --sourceRoot=./ --copy-files --sourcemaps",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run prepare; npm run build; npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^5.0.0",
    "mup": "^1.3.7",
    "shelljs": "^0.7.8"
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "aws-sdk": "^2.145.0",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "ejs": "^2.5.7",
    "joi": "^10.6.0",
    "lodash": "^4.17.4",
    "random-seed": "^0.3.0",
    "tar": "^4.0.2",
    "uuid": "^3.1.0"
  }
}
